                                 referencedTableName="classe"/>

        <addForeignKeyConstraint baseColumnNames="encadreur_id"
                                 baseTableName="etudiant"
                                 constraintName="fk_etudiant__encadreur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="encadreur"/>
    </changeSet>
</databaseChangeLog>
